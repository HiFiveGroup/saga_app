<section class="main">
  <h1>Listing users</h1>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th></th>
        <th>Image url</th>
        <th></th>
        <th>City</th>
        <th>&nbsp;&nbsp;&nbsp;&nbsp;</th>
        <th>State</th>
        <th>&nbsp;&nbsp;&nbsp;&nbsp;</th>
        <th>Country</th>
        <!-- <th colspan="3"></th> -->
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= link_to(user.name, user_path(user.id)) %></td>
          <td>&nbsp;&nbsp;&nbsp;&nbsp;</td>
          <td><img src="<%= user.image_url %>"width="400"></td>
          <td>&nbsp;&nbsp;&nbsp;&nbsp;</td>
          <td><%= user.city %></td>
          <td>&nbsp;&nbsp;&nbsp;&nbsp;</td>
          <td><%= user.state %></td>
          <td>&nbsp;&nbsp;&nbsp;&nbsp;</td>
          <td><%= user.country %></td>
          <!-- <td><%= link_to 'Show', user %></td>
          <td><%= link_to 'Edit', edit_user_path(user) %></td>
          <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
        </tr>
      <% end %>
      <br>
    </tbody>
  </table>
  <%= will_paginate @users %>
</section>